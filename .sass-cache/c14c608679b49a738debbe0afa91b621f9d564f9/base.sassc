input
  &[type="submit"]:active, &[type="reset"]:active, &[type="button"]:active
    border: 1px solid #666
    background: #ccc
    /* Old browsers
    background: -moz-linear-gradient(top, rgba(255, 255, 255, 0.35) 0%, rgba(10, 10, 10, 0.4) 100%)
    /* FF3.6+
